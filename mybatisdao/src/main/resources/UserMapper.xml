<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="awin.user.mapper.UserMapper">

    <select id="selectByPrimaryKey" parameterType="java.lang.String" resultType="awin.user.mapper.UserVO">
        select cuserid,user_name
        from sm_user
        <where></where>
        where cuserid = #{pk,jdbcType=CHAR}
    </select>


</mapper>